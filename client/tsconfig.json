{
    "compilerOptions": {
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "noFallthroughCasesInSwitch": true,
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "jsx": "react-native",
        "lib": ["es6"],
        "moduleResolution": "node",
        "skipLibCheck": true,
        "noEmit": true,
        "strict": true,
        "target": "esnext"
    },
    "exclude": ["node_modules"]
}
