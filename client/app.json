{
    "expo": {
        "name": "Ryazan Mobile Application",
        "slug": "ryazan",
        "privacy": "public",
        "sdkVersion": "33.0.0",
        "platforms": ["ios", "android"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "bundleIdentifier": "org.mobile.ryazan",
            "supportsTablet": true,
            "infoPlist": {
                "NSAllowsArbitraryLoads": {
                    "NSAllowsArbitraryLoads": true
                }
            }
        },
        "android": {
            "package": "org.mobile.ryazan"
        },
        "extra": {
            "apiUrl": "http://192.168.0.200:1337/graphql"
        }
    }
}
