{
  "connection": "default",
  "collectionName": "places",
  "info": {
    "name": "poi",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "longitude": {
      "type": "float",
      "required": true
    },
    "category": {
      "model": "category",
      "via": "pois"
    },
    "description": {
      "type": "text"
    },
    "routeitems": {
      "collection": "routeitem",
      "via": "poi"
    },
    "photos": {
      "collection": "photo",
      "via": "poi"
    }
  }
}